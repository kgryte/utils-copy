    "object",
    "date",
    "regexp",
    "typed array",
    "cp"
  ],
  "bugs": {
    "url": "https://github.com/kgryte/utils-copy/issues"
  },
  "dependencies": {
    "type-name": "^1.1.0",
    "utils-regex-from-string": "^1.0.0",
    "validate.io-array": "^1.0.3",
